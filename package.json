{
    "name": "@bloxroute/solana-trader-client-ts",
    "version": "1.3.5",
    "description": "Solana Trader API SDK",
    "main": "dist/index.js",
    "directories": {
        "example": "example"
    },
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "npm run clean && node build.js",
        "clean": "rm -rf dist",
        "test": "echo \"No test specified\"",
        "release": "npm run build && np --no-2fa --any-branch",
        "start": "ts-node ./examples/index.ts",
        "lintCI": "eslint --max-warnings 0 .",
        "lint": "eslint --fix .",
        "format": "prettier --config .prettierrc -l './**/*.ts' --write",
        "formatCI": "prettier --config .prettierrc -l './**/*.ts'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bloXroute-Labs/solana-trader-client-ts.git"
    },
    "author": "bloXroute",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bloXroute-Labs/solana-trader-client-ts/issues"
    },
    "homepage": "https://github.com/bloXroute-Labs/solana-trader-client-ts#readme",
    "dependencies": {
        "@grpc/grpc-js": "1.8.1",
        "@solana/web3.js": "^1.47.3",
        "axios": "1.2.2",
        "bs58": "^5.0.0",
        "dotenv": "^16.0.1",
        "google-protobuf": "^3.20.1",
        "grpc-web": "1.4.2",
        "isomorphic-ws": "5.0.0",
        "np": "^6.0.0",
        "pbkit": "^0.0.53",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4",
        "websocket-iterator": "^1.0.1",
        "ws": "^8.8.0"
    },
    "devDependencies": {
        "@types/google-protobuf": "^3.15.6",
        "@types/node": "^18.8.4",
        "@types/node-fetch": "^2.6.2",
        "@types/ws": "^8.5.3",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "esbuild": "0.16.14",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.1",
        "npm-dts": "1.3.12",
        "prettier": "^2.7.1",
        "ts-proto": "^1.115.5"
    }
}
